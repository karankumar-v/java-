import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
 Scanner num=new Scanner(System.in);
        int a=num.nextInt();
        if(a==0){
            System.out.println("zero");}
            else if(a==12){
                System.out.println("positive");
            }
        else{
            System.out.println("negative");
        }
            
        
    }
}
