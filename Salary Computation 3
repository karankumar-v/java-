import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sal=new Scanner(System.in);
        int a=sal.nextInt();
        double hra=a*0.50;
        double da=a*0.55;
        double gs=a+hra+da;
        System.out.printf("%.2f",gs);
    }
}
