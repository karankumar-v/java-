import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
Scanner dis=new Scanner(System.in);
        int x1=dis.nextInt();
         
         int y1=dis.nextInt();
        int x2=dis.nextInt();
         int y2=dis.nextInt();
        double mida=(x1 + x2)/2.0;
        double midb=(y1 + y2)/2.0;
        System.out.println("Binoy's house is located at (" + mida + "," + midb +")");
        
    }
}
